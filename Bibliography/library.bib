number = {6},
pages = {3208--3218},
pmid = {25735748},
title = {{U1 snRNP is mislocalized in ALS patient fibroblasts bearing NLS mutations in FUS and is required for motor neuron outgrowth in zebrafish}},
volume = {43},
year = {2015}
}
