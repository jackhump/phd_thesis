isbn = {0022-3050},
issn = {1468-330X},
journal = {Journal of neurology, neurosurgery, and psychiatry},
keywords = {Adult,Aged,Amyotrophic Lateral Sclerosis,Amyotrophic Lateral Sclerosis: epidemiology,Europe,Europe: epidemiology,Female,Humans,Incidence,Male,Middle Aged,Prospective Studies},
number = {4},
