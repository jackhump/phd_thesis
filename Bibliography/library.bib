@article{Andersson2008,
abstract = {BACKGROUND: FUS, EWS and TAF15 are structurally similar multifunctional proteins that were first discovered upon characterization of fusion oncogenes in human sarcomas and leukemias. The proteins belong to the FET (previously TET) family of RNA-binding proteins and are implicated in central cellular processes such as regulation of gene expression, maintenance of genomic integrity and mRNA/microRNA processing. In the present study, we investigated the expression and cellular localization of FET proteins in multiple human tissues and cell types.$\backslash$n$\backslash$nRESULTS: FUS, EWS and TAF15 were expressed in both distinct and overlapping patterns in human tissues. The three proteins showed almost ubiquitous nuclear expression and FUS and TAF15 were in addition present in the cytoplasm of most cell types. Cytoplasmic EWS was more rarely detected and seen mainly in secretory cell types. Furthermore, FET expression was downregulated in differentiating human embryonic stem cells, during induced differentiation of neuroblastoma cells and absent in terminally differentiated melanocytes and cardiac muscle cells. The FET proteins were targeted to stress granules induced by heat shock and oxidative stress and FUS required its RNA-binding domain for this translocation. Furthermore, FUS and TAF15 were detected in spreading initiation centers of adhering cells.$\backslash$n$\backslash$nCONCLUSION: Our results point to cell-specific expression patterns and functions of the FET proteins rather than the housekeeping roles inferred from earlier studies. The localization of FET proteins to stress granules suggests activities in translational regulation during stress conditions. Roles in central processes such as stress response, translational control and adhesion may explain the FET proteins frequent involvement in human cancer.},
annote = {FUS is localised to the nucleus in all tissues},
author = {Andersson, Mattias K. and St{\aa}hlberg, Anders and Arvidsson, Yvonne and Olofsson, Anita and Semb, Henrik and Stenman, G{\"{o}}ran and Nilsson, Ola and {\AA}man, Pierre},
doi = {10.1186/1471-2121-9-37},
file = {:Users/jack/Downloads/Papers/1471-2121-9-37.pdf:pdf},
