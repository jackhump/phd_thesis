keywords = {Cutoff criteria,D-dimer,Recurrence,Venous thromboembolism},
number = {1},
pages = {42--49},
title = {{Linking Amyotrophic Lateral Sclerosis and Spinal Muscular Atrophy through RNA-transcriptome homeostasis: a genomics perspective}},
