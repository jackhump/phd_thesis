isbn = {1546-1696 (Electronic)$\backslash$r1087-0156 (Linking)},
issn = {15461696},
journal = {Nature Biotechnology},
number = {7},
pages = {736--742},
pmid = {25985263},
title = {{Comprehensive transcriptome analysis using synthetic long-read sequencing reveals molecular co-association of distant splicing events}},
volume = {33},
year = {2015}
}
