journal = {Acta Neuropathologica},
keywords = {Amyotrophic lateral sclerosis (ALS),Microglia,Motor neuron disease (MND),Neurodegeneration,Pathological spreading,TDP-43,Zebrafish},
number = {3},
pages = {445--459},
