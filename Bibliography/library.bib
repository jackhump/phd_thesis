issn = {10974172},
journal = {Cell},
keywords = {FUS,PY-NLS,RNA granule,amyotrophic lateral sclerosis,biomolecular condensate,intrinsically disordered protein,karyopherin-$\beta$2,liquid-liquid phase separation,low-complexity sequences,transportin-1},
