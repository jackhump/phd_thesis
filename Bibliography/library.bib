isbn = {8863758646},
issn = {1355-8382},
journal = {Rna},
pages = {rna.066951.118},
pmid = {29959282},
title = {{An important class of intron retention events in human erythroblasts is regulated by cryptic exons proposed to function as splicing decoys}},
url = {http://rnajournal.cshlp.org/lookup/doi/10.1261/rna.066951.118},
year = {2018}
