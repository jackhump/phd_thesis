file = {:Users/jack/Library/Application Support/Mendeley Desktop/Downloaded/Rosen et al. - 1993 - Mutations in CuZn superoxide dismutase gene are associated with familial amyotrophic lateral sclerosis.pdf:pdf},
isbn = {0028-0836},
