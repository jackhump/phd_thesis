issn = {14712121},
journal = {BMC Cell Biology},
pages = {1--17},
pmid = {18620564},
title = {{The multifunctional FUS, EWS and TAF15 proto-oncoproteins show cell type-specific expression patterns and involvement in cell spreading and stress response}},
volume = {9},
year = {2008}
}
