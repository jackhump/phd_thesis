file = {:Users/jack/Library/Application Support/Mendeley Desktop/Downloaded/Onyike, Diehl-Schmid - 2013 - The epidemiology of frontotemporal dementia.pdf:pdf},
isbn = {1369-1627 (Electronic)$\backslash$r0954-0261 (Linking)},
issn = {1369-1627},
journal = {International review of psychiatry (Abingdon, England)},
number = {2},
pages = {130--7},
pmid = {23611343},
title = {{The epidemiology of frontotemporal dementia.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/23611343{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3932112},
volume = {25},
