@article{Ocklenburg2017,
abstract = {{\textless}p{\textgreater}Lateralization is a fundamental principle of nervous system organization but its molecular determinants are mostly unknown. In humans, asymmetric gene expression in the fetal cortex has been suggested as the molecular basis of handedness. However, human fetuses already show considerable asymmetries in arm movements before the motor cortex is functionally linked to the spinal cord, making it more likely that spinal gene expression asymmetries form the molecular basis of handedness. We analyzed genome-wide mRNA expression and DNA methylation in cervical and anterior thoracal spinal cord segments of five human fetuses and show development-dependent gene expression asymmetries. These gene expression asymmetries were epigenetically regulated by miRNA expression asymmetries in the TGF-$\beta$ signaling pathway and lateralized methylation of CpG islands. Our findings suggest that molecular mechanisms for epigenetic regulation within the spinal cord constitute the starting point for handedness, implying a fundamental shift in our understanding of the ontogenesis of hemispheric asymmetries in humans.{\textless}/p{\textgreater}},
author = {Ocklenburg, Sebastian and Schmitz, Judith and Moinfar, Zahra and Moser, Dirk and Klose, Rena and Lor, Stephanie and Kunz, Georg and Tegenthoff, Martin and Faustmann, Pedro and Francks, Clyde and Epplen, J{\"{o}}rg T and Kumsta, Robert and G{\"{u}}nt{\"{u}}rk{\"{u}}n, Onur},
doi = {10.7554/eLife.22784},
file = {:Users/jack/Library/Application Support/Mendeley Desktop/Downloaded/Ocklenburg et al. - 2017 - Epigenetic regulation of lateralized fetal spinal gene expression underlies hemispheric asymmetries.pdf:pdf},
issn = {2050-084X},
journal = {eLife},
pages = {1--19},
pmid = {28145864},
title = {{Epigenetic regulation of lateralized fetal spinal gene expression underlies hemispheric asymmetries}},
url = {http://elifesciences.org/lookup/doi/10.7554/eLife.22784},
volume = {6},
