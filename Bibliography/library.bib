journal = {Molecular and Cellular Biology},
number = {6},
pages = {1442--1451},
pmid = {19124611},
title = {{Auto- and Cross-Regulation of the hnRNP L Proteins by Alternative Splicing}},
url = {http://mcb.asm.org/cgi/doi/10.1128/MCB.01689-08},
volume = {29},
year = {2009}
}
