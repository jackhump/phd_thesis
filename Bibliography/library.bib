keywords = {cies,markov models,maximal dependence decomposition,maximum entropy,molecular sequence analysis,non-neighboring dependen-,regulatory elements,splice sites},
number = {2-3},
pages = {377--394},
pmid = {15285897},
title = {{Maximum entropy modeling of short sequence motifs with applications to RNA splicing signals.}},
volume = {11},
