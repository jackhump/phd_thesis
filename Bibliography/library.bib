issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {11},
pages = {5740--5745},
pmid = {10811881},
title = {{The tetranucleotide UCAY directs the specific recognition of RNA by the Nova K-homology 3 domain}},
