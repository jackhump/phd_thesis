isbn = {0261-4189},
issn = {02614189},
journal = {EMBO Journal},
keywords = {Transportin,amyotrophic lateral sclerosis (ALS),frontotemporal lobar degeneration (FTLD),fused in sarcoma (FUS),stress granules},
