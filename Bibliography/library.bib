issn = {0890-9369},
journal = {Genes {\&} Development},
keywords = {2014,2015,22,alternative polyadenylation,clip,for this article,fus,fused in sarcoma,mrna length,received november 11,revised version accepted april,rna polymerase ii,supplemental material is available,tls,translocated in liposarcoma},
