journal = {Human Mutation},
keywords = {Alzheimer,Frontotemporal dementia,Linkage,MAPT,Microtubule,Pick,Tau},
number = {4},
pages = {277--295},
pmid = {15365985},
title = {{The role of tau (MAPT) in frontotemporal dementia and related tauopathies}},
volume = {24},
year = {2004}
}
