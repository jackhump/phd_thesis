journal = {Nature communications},
number = {May},
pages = {7098},
pmid = {25968143},
publisher = {Nature Publishing Group},
title = {{FUS regulates AMPA receptor function and FTLD/ALS-associated behaviour via GluA1 mRNA stabilization.}},
url = {http://www.nature.com/ncomms/2015/150513/ncomms8098/full/ncomms8098.html?WT.ec{\_}id=NCOMMS-20150520{\&}spMailingID=48696510{\&}spUserID=NTc3MjA3Njg0NTcS1{\&}spJobID=682450483{\&}spReportId=NjgyNDUwNDgzS0},
volume = {6},
