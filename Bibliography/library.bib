pmid = {20110278},
title = {{BEDTools: a flexible suite of utilities for comparing genomic features.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2832824{\&}tool=pmcentrez{\&}rendertype=abstract{\%}5Cnhttp://bioinformatics.oxfordjournals.org/content/26/6/841.short},
volume = {26},
