issn = {1879-0445},
journal = {Current biology : CB},
keywords = {CHD2,Chd1,Drosophila,H3K4me3,TDP-43,amyotrophic lateral sclerosis,chromatin remodeling,epigenetics,frontotemporal dementia,heat shock proteins},
number = {0},
pages = {3579--3590},
