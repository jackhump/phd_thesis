title = {{Major hnRNP proteins act as general TDP-43 functional modifiers both in Drosophila and human neuronal cells.}},
url = {https://academic.oup.com/nar/article-lookup/doi/10.1093/nar/gkx477{\%}0Ahttp://www.ncbi.nlm.nih.gov/pubmed/28575377},
year = {2017}
