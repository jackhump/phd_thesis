issn = {10974172},
journal = {Cell},
keywords = {ALS,FTD,Karyopherin-$\beta$2 (Kap$\beta$2),Transportin (TNPO1),arginine methylation,fused in sarcoma (FUS),neurodegeneration,nuclear import,phase separation,stress granules},
number = {3},
pages = {706--719.e13},
pmid = {29677514},
publisher = {Elsevier Inc.},
title = {{Phase Separation of FUS Is Suppressed by Its Nuclear Import Receptor and Arginine Methylation}},
