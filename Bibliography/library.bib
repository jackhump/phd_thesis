isbn = {0040101616038},
issn = {14320533},
journal = {Acta Neuropathologica},
keywords = {CYFIP1,FISH,FMRP,Immunofluorescence staining,Live cell imaging,Polysome profile,RNA-IP,TDP-43,Translation initiation},
number = {5},
pages = {721--738},
pmid = {27518042},
