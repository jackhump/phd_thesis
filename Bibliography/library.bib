title = {{TDP-43 loss of function inhibits endosomal trafficking and alters trophic signaling in neurons.}},
url = {http://emboj.embopress.org/lookup/doi/10.15252/embj.201694221{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/27621269{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC5090220},
volume = {35},
