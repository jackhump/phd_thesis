	keywords = {Alu,Hyperphenylalaninemia,Long interspersed nuclear element,PTS,Tetrahydrobiopterin},
	number = {5},
	pages = {823--831},
	pmid = {19280650},
	title = {{Disease-causing mutations improving the branch site and polypyrimidine tract: Pseudoexon activation of LINE-2 and antisense alu lacking the poly(T)-Tail}},
