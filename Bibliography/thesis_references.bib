@ARTICLE{Van_Nostrand2016-su,
  title    = "Robust transcriptome-wide discovery of {RNA-binding} protein
              binding sites with enhanced {CLIP} ({eCLIP})",
  author   = "Van Nostrand, Eric L and Pratt, Gabriel A and Shishkin, Alexander
