	doi = {10.1371/journal.pbio.1000530},
	file = {:Users/jack/Downloads/Papers/journal.pbio.1000530.PDF:PDF},
	isbn = {1545-7885; 1544-9173},
	issn = {15457885},
	journal = {PLoS Biology},
	number = {10},
	pmid = {21048981},
	title = {{iCLIP predicts the dual splicing effects of TIA-RNA interactions}},
	volume = {8},
	year = {2010}
}

