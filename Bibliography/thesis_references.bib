	journal = {Genome biology},
	keywords = {Binding Sites,DNA Transposable Elements,Gene Regulatory Networks,Humans,K562 Cells,Molecular Sequence Data,RNA,RNA Processing, Post-Transcriptional,RNA Splicing,RNA-Binding Proteins,RNA-Binding Proteins: chemistry,RNA-Binding Proteins: genetics,RNA-Binding Proteins: metabolism,RNA: metabolism},
	number = {12},
	pages = {537},
	pmid = {25572935},
