	title = {{TLS facilitates transport of mRNA encoding an actin-stabilizing protein to dendritic spines}},
	NOOPurl = {/Users/yurikoharigaya/Documents/ReadCube Media/fujii2005b.pdf{\%}5Cnhttp://dx.doi.org/10.1242/jcs.02692},
	volume = {118},
