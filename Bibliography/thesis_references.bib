@article{Lopez-Erauskin2018,
	abstract = {{\textless}h2{\textgreater}Summary{\textless}/h2{\textgreater}{\textless}p{\textgreater}Through the generation of humanized FUS mice expressing full-length human FUS, we identify that when expressed at near endogenous murine FUS levels, both wild-type and ALS-causing and frontotemporal dementia (FTD)-causing mutations complement the essential function(s) of murine FUS. Replacement of murine FUS with mutant, but not wild-type, human FUS causes stress-mediated induction of chaperones, decreased expression of ion channels and transporters essential for synaptic function, and reduced synaptic activity without loss of nuclear FUS or its cytoplasmic aggregation. Most strikingly, accumulation of mutant human FUS is shown to activate an integrated stress response and to inhibit local, intra-axonal protein synthesis in hippocampal neurons and sciatic nerves. Collectively, our evidence demonstrates that human ALS/FTD-linked mutations in FUS induce a gain of toxicity that includes stress-mediated suppression in intra-axonal translation, synaptic dysfunction, and progressive age-dependent motor and cognitive disease without cytoplasmic aggregation, altered nuclear localization, or aberrant splicing of FUS-bound pre-mRNAs.{\textless}/p{\textgreater}},
	annote = {FUS mutations downregulate translation - cite in FUS mice chapter},
	author = {L{\'{o}}pez-Erauskin, Jone and Tadokoro, Takahiro and Baughn, Michael W. and Myers, Brian and McAlonis-Downes, Melissa and Chillon-Marinas, Carlos and Asiaban, Joshua N. and Artates, Jonathan and Bui, Anh T. and Vetto, Anne P. and Lee, Sandra K. and Le, Ai Vy and Sun, Ying and Jambeau, M{\'{e}}lanie and Boubaker, Jihane and Swing, Deborah and Qiu, Jinsong and Hicks, Geoffrey G. and Ouyang, Zhengyu and Fu, Xiang-Dong and Tessarollo, Lino and Ling, Shuo-Chien and Parone, Philippe A. and Shaw, Christopher E. and Marsala, Martin and Lagier-Tourenne, Clotilde and Cleveland, Don W. and {Da Cruz}, Sandrine},
	doi = {10.1016/j.neuron.2018.09.044},
	file = {:Users/jack/Downloads/Papers/Lopez-Erauskin et al{\%}2c FUS mutation Suppresses Intraaxonal protein synthesis and drives disaese without nuclear LOF{\%}2c Neuron 2018.pdf:pdf},
	issn = {08966273},
	journal = {Neuron},
	keywords = {ALS,Amyotrophic Lateral Sclerosis,FTD,FUS,Frontotemporal dementia,RNA-binding proteins,TLS,axonal translation,neurodegenerative disease,protein synthesis},
	number = {0},
	pages = {1--15},
	pmid = {30344044},
	title = {{ALS/FTD-Linked Mutation in FUS Suppresses Intra-axonal Protein Synthesis and Drives Disease Without Nuclear Loss-of-Function of FUS}},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S0896627318308468},
	volume = {0},
