@article{Ip2011,
	abstract = {The rate of RNA polymerase II (Pol II) elongation can influence splice site selection in nascent transcripts, yet the extent and physiological relevance of this kinetic coupling between transcription and alternative splicing (AS) is not well understood. We performed experiments to perturb Pol II elongation and then globally compared AS patterns with genome-wide Pol II occupancy. RNA binding and RNA processing functions were significantly enriched among the genes with Pol II elongation inhibition-dependent changes in AS. Under conditions that interfere with Pol II elongation, including cell stress, increased Pol II occupancy was detected in the intronic regions flanking the alternative exons in these genes, and these exons generally became more included. A disproportionately high fraction of these exons introduced premature termination codons that elicited nonsense-mediated mRNA decay (NMD), thereby further reducing transcript levels. Our results provide evidence that kinetic coupling between transcription, AS, and NMD affords a rapid mechanism by which cells can respond to changes in growth conditions, including cell stress, to coordinate the levels of RNA processing factors with mRNA levels.},
	annote = {Reducing Pol II elongation activity leads to increased PTC exon inclusion and reduced transcript levels for RNA binding and apoptosis genes
	
	Splicing is co-transcriptional - happens as the transcript is produced. 
	What links transcription to splicing?
	Splicing factors!
	
	Recruitment model - splicing factors bind to RNA sequence and recruit transcriptional modifiers 
	Kinetic model - transcriptional modifiers regulate splicing by altering transcription speed. This would alter splicing decisions by making splice sites more accessible
	
	Alternate exons have weaker splice sites than constitutitve exons. Pol II stalling in the intron gives more time for weak splice sites to be recognised, or for weak silencing elements to be bound.},
	author = {Ip, Joanna Y. and Schmidt, Dominic and Pan, Qun and Ramani, Arun K. and Fraser, Andrew G. and Odom, Duncan T. and Blencowe, Benjamin J.},
	doi = {10.1101/gr.111070.110},
	file = {:Users/jack/Downloads/Papers/Genome Res.-2011-Ip-390-401.pdf:pdf},
	isbn = {1549-5469 (Electronic)$\backslash$n1088-9051 (Linking)},
	issn = {10889051},
	journal = {Genome Research},
	number = {3},
	pages = {390--401},
	pmid = {21163941},
	title = {{Global impact of RNA polymerase II elongation inhibition on alternative splicing regulation}},
	volume = {21},
	year = {2011}
}

@article{Kornblihtt2004a,
	abstract = {Transcription and pre-mRNA splicing are extremely complex multimolecular processes that involve protein-DNA, protein-RNA, and protein-protein interactions. Splicing occurs in the close vicinity of genes and is frequently cotranscriptional. This is consistent with evidence that both processes are coordinated and, in some cases, functionally coupled. This review focuses on the roles of cis- and trans-acting factors that regulate transcription, on constitutive and alternative splicing. We also discuss possible functions in splicing of the C-terminal domain (CTD) of the RNA polymerase II (pol II) largest subunit, whose participation in other key pre-mRNA processing reactions (capping and cleavage/polyadenylation) is well documented. Recent evidence indicates that transcriptional elongation and splicing can be influenced reciprocally: Elongation rates control alternative splicing and splicing factors can, in turn, modulate pol II elongation. The presence of transcription factors in the spliceosome and the existence of proteins, such as the coactivator PGC-1, with dual activities in splicing and transcription can explain the links between both processes and add a new level of complexity to the regulation of gene expression in eukaryotes.},
	author = {Kornblihtt, Alberto R. and {De La Mata}, Manuel and Fededa, Juan Pablo and Mu{\~{n}}oz, Manuel J. and Nogu{\'{e}}s, Guadalupe},
	doi = {10.1261/rna.7100104},
	file = {:Users/jack/Downloads/Papers/RNA-2004-KORNBLIHTT-1489-98 (1).pdf:pdf},
	isbn = {1355-8382 (Print)$\backslash$n1355-8382 (Linking)},
	issn = {13558382},
	journal = {Rna},
	keywords = {Alternative splicing,Coupling,RNA polymerase II,Splicing,Transcription},
