	isbn = {1558-1497 (Electronic)$\backslash$r0197-4580 (Linking)},
	issn = {01974580},
	journal = {Neurobiology of Aging},
	keywords = {Amyotrophic lateral sclerosis,FUS gene,Family pedigrees,Genetics},
