	issn = {15353907},
	journal = {Journal of Proteome Research},
	keywords = {LC-MS/MS,amyotrophic lateral sclerosis,biomarker,cerebrospinal fluid,extracellular matrix,liquid chromatography-tandem mass spectrometry,mass spectrometry,proteomics},
	number = {11},
	pages = {4486--4501},
	pmid = {26401960},
	title = {{Label-free LC-MS/MS proteomic analysis of cerebrospinal fluid identifies protein/pathway alterations and candidate biomarkers for amyotrophic lateral sclerosis}},
