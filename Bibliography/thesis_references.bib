@article{Bai2015,
	abstract = {Background: The emergence of next-generation RNA sequencing (RNA-Seq) provides tremendous opportunities for researchers to analyze alternative splicing on a genome-wide scale. However, accurate detection of intron retention (IR) events from RNA-Seq data has remained an unresolved challenge in next-generation sequencing (NGS) studies. Results: We propose two new methods: IRcall and IRclassifier to detect IR events from RNA-Seq data. Our methods combine together gene expression information, read coverage within an intron, and read counts (within introns, within flanking exons, supporting splice junctions, and overlapping with 5' splice site/ 3' splice site), employing ranking strategy and classifiers to detect IR events. We applied our approaches to one published RNA-Seq data on contrasting skip mutant and wild-type in Arabidopsis thaliana. Compared with three state-of-the-art methods, IRcall and IRclassifier could effectively filter out false positives, and predict more accurate IR events. Availability: The data and codes of IRcall and IRclassifier are available at http://mlg.hit.edu.cn/ybai/IR/ IRcallAndIRclass.html},
	author = {Bai, Yang and Ji, Shufan and Wang, Yadong},
	doi = {10.1186/1471-2164-16-S2-S9},
	file = {:Users/Jack/Downloads/1471-2164-16-S2-S9.pdf:pdf},
	issn = {1471-2164},
	journal = {BMC Genomics},
	number = {Suppl 2},
