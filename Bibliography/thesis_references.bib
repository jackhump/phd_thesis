	keywords = {3'UTR,Alternative polyadenylation,Diversification of protein functions,Multifunctionality,Noncoding RNA,Post-transcriptional gene regulation,Protein abundance,Protein localization,Protein-protein interactions,RNA granule,RNA-binding protein},
