	isbn = {9780874216561},
	issn = {0305-1048},
	journal = {Nucleic Acids Research},
	keywords = {high resolution images,research,risks management,sustainable reconstruction},
	month = {may},
	number = {23},
	pages = {7461--7475},
	pmid = {15991970},
	title = {{Ubiquitous transposon-like repeats B1 and B2 of the mouse genome: B2 sequencing}},
	NOOPurl = {http://www.ncbi.nlm.nih.gov/pubmed/15991970 http://nar.oxfordjournals.org/lookup/doi/10.1093/nar/10.23.7461},
	volume = {10},
