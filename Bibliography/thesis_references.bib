	number = {2},
	pages = {324--339.e23},
	pmid = {28709000},
	publisher = {Elsevier Inc.},
	title = {{Regulatory Expansion in Mammals of Multivalent hnRNP Assemblies that Globally Control Alternative Splicing}},
