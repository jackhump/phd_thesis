	journal = {PLoS Genetics},
	number = {1},
	pages = {1--32},
	pmid = {25611934},
	title = {{hnRNP K Coordinates Transcriptional Silencing by SETDB1 in Embryonic Stem Cells}},
	volume = {11},
	year = {2015}
}
