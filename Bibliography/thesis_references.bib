	issn = {2211-1247},
	journal = {Cell Reports},
	number = {5},
	pages = {1--9},
	publisher = {The Authors},
	title = {{In Vivo Formation of Vacuolated Multi-phase Compartments Lacking Membranes Cell Reports Report In Vivo Formation of Vacuolated Multi-phase Compartments Lacking Membranes}},
	NOOPurl = {http://dx.doi.org/10.1016/j.celrep.2016.06.088},
