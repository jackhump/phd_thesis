	file = {:Users/jack/Downloads/Papers/rosenfeld2002.pdf:pdf},
	isbn = {0022-2836 (Print)$\backslash$r0022-2836 (Linking)},
	issn = {00222836},
	journal = {Journal of Molecular Biology},
	keywords = {Autogenous control,Biophysics,Design principles,Feedback kinetics,Systems-biology},
	number = {5},
	pages = {785--793},
	pmid = {12417193},
