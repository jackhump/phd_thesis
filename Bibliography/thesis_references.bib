	journal = {Rna},
	keywords = {Alternative splicing,Cryptic exons,Dinucleotide repeats,HNRNPL,HnRNP L},
	number = {6},
	pages = {761--768},
	pmid = {29581412},
	title = {{HnRNP L represses cryptic exons}},
	volume = {24},
	year = {2018}
}

