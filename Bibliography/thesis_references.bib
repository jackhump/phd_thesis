	journal = {Neuroscience},
	keywords = {Amyotrophic lateral sclerosis,Comparative proteomics,Frontotemporal lobar degeneration,Intracellular transport,RanBP1,TDP-43},
	pages = {157--170},
	pmid = {25743254},
	publisher = {IBRO},
	title = {{Proteomic analyses reveal that loss of TDP-43 affects RNA processing and intracellular transport}},
