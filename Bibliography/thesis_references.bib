	issn = {10597794},
	journal = {Human Mutation},
	keywords = {ATM,Ataxia-telangiectasia,Mutation screening, splicing, entropy modeling},
	number = {1},
	pages = {67--76},
	pmid = {14695534},
	title = {{Nonclassical Splicing Mutations in the Coding and Noncoding Regions of the ATM Gene: Maximum Entropy Estimates of Splice Junction Strengths}},
	volume = {23},
	year = {2004}
}
